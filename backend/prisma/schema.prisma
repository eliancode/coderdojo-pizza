generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model order {
  id         Int      @id @default(autoincrement())
  personName String
  orderName  String
  orderSize  String
  orderPrice String
  createdAt  DateTime @default(now())
  archived   Boolean  @default(false)
  paid       Boolean  @default(false)
}
